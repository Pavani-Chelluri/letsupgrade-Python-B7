   "metadata": {},
   "outputs": [],
   "source": [
    "n1=1042000\n",
    "n2=702648265\n",
    "for num in range(n1,n2+1):\n",
    "    n=num\n",
    "    d=len(str(n))\n",
    "    s=0\n",
    "    while(n>0):\n",
    "        \n",
    "        t=n%10\n",
    "        s=s+(t**d)\n",
    "        n=n//10\n",
    "    if(num==s):\n",
    "        print(num)\n",
    "    break"
   ]
